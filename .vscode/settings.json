{
    "files.associations": {
        "file_handler.h": "c",
        "deduplication.h": "c",
        "backup_manager.h": "c",
        "stat.h": "c",
        "md5.h": "c"
    },
    "editor.insertSpaces": true,
    "editor.tabSize": 4,
    "editor.detectIndentation": false,
    "C_Cpp.clang_format_style": "{ BasedOnStyle: Google, AllowAllArgumentsOnNextLine: false, IndentWidth: 4, ColumnLimit: 0, PointerAlignment: Right, SpaceBeforeParens: ControlStatements, SpacesBeforeTrailingComments: 1, SpaceAfterCStyleCast: false }",
    "[c]": {
        "editor.defaultFormatter": "ms-vscode.cpptools",
        "editor.formatOnSave": true
    },
    "editor.rulers": [100],
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "C_Cpp.default.cStandard": "c17"
}
